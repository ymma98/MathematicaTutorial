Cell[TextData[{
 StyleBox["Clear, ClearAll, Remove",
  FontSlant->"Italic"],
 " (",
 StyleBox["Clear",
  FontSlant->"Italic"],
 " means to clear the Rules (",
 StyleBox["OwnValues",
  FontSlant->"Italic"],
 ", ",
 StyleBox["UpValues",
  FontSlant->"Italic"],
 ", ",
 StyleBox["DownValues",
  FontSlant->"Italic"],
 " ... ) attached with the symbol. ",
 StyleBox["ClearAll",
  FontSlant->"Italic"],
 " means to clear the the Rules, ",
 StyleBox["Options",
  FontSlant->"Italic"],
 ", and ",
 StyleBox["Attributes",
  FontSlant->"Italic"],
 " attached with the symbol. ",
 StyleBox["Remove",
  FontSlant->"Italic"],
 " means to remove the symbol from the symbol table (do not use ",
 StyleBox["Remove",
  FontSlant->"Italic"],
 "...Because its mechanism is not very clear. )\[LineSeparator]e.g. ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"ClearAll", "/@", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"$Context", "<>", "\"\<*\>\""}], ")"}], "//", "Names"}], 
     ")"}]}], ";"}]], "Input",ExpressionUUID->
  "dd1353c6-2c96-40d3-b0dd-d66caaf52940"],
 "      )"
